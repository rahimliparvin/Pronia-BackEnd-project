@using ProniaBackEndProject.Areas.Admin.ViewModels.ContactVM;
@model ContactDetailVM
@{
    ViewData["Title"] = "Detail";
}

<div class="container" style="padding-left:200px">

    <div class="form-group my-3">
        <label>NAME</label>
        <input disabled class="form-control" value="@Model.FirstName" style="display:block;" />
    </div>

    <div class="form-group  my-3">
        <label>LastName</label>
        <input disabled class="form-control" value="@Model.LastName" style="display:block;" />
    </div>

    <div class="form-group  my-3">
        <label>Email</label>
        <input disabled class="form-control" value="@Model.Email" style="display:block;" />
    </div>

    <div class="form-group  my-3">
        <label>Phone</label>
        <input disabled class="form-control" value="@Model.Phone" style="display:block;" />
    </div>

    <div class="form-group  my-3">
        <label>Message</label>
        <input disabled class="form-control" value="@Model.Message" style="display:block;" />
    </div>
   
    <a style=" float: right;" class="btn btn-info my-3" asp-action="Index">Back</a>

</div>

